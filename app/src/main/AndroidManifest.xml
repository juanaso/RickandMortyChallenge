<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.juanasoapp.rickandmortychallenge">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_rick"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_rick_round"
        android:supportsRtl="true"
        android:name="com.juanasoapp.rickandmortychallenge.core.RickAndMortyChallengeApplication"
        android:theme="@style/AppTheme">
        <activity android:name="com.juanasoapp.rickandmortychallenge.MainActivity"
            android:label="@string/app_display_title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>